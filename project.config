[project]
	description = System configuration validation tool
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group citellus-core
	owner = group user/Robin Černín (zerodayz)
[access "refs/for/refs/*"]
	read = group user/Robin Černín (zerodayz)
	push = group user/Robin Černín (zerodayz)
	label-Code-Review = -2..+2 group citellus-core
	label-Code-Review = -2..+2 group user/Robin Černín (zerodayz)
	label-Verified = -1..+1 group user/Robin Černín (zerodayz)
	rebase = group user/Robin Černín (zerodayz)
	removeReviewer = group user/Robin Černín (zerodayz)
	submit = group user/Robin Černín (zerodayz)
[access "refs/heads/*"]
	read = group user/Robin Černín (zerodayz)
	create = group user/Robin Černín (zerodayz)
	push = +force group user/Robin Černín (zerodayz)
	pushMerge = group user/Robin Černín (zerodayz)
[access "refs/tags/*"]
	push = group user/Robin Černín (zerodayz)
	push = +force group user/Robin Černín (zerodayz)
